{
  "compilerOptions": {
    "target": "es5",
    "lib": [ "es6", "dom" ],
    "experimentalDecorators": true,
    "jsx": "react",
    "outDir": "dist",
    "strict": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}